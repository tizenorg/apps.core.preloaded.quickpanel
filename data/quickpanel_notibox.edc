			target: "background";
			after: "box.landscape.custom";
		}
		program{
			name: "box.portrait.custom";
			source: "box.prog";
			script {
				if (get_int(g_is_show_dim_bg) == 1) {
					set_state(PART:"object.icon.bg.dim", "portrait", 0.0);
				}
			}
		}
		program{
			name: "box.landscape.custom";
			source: "box.prog";
			script {
				if (get_int(g_is_show_dim_bg) == 1) {
					set_state(PART:"object.icon.bg.dim", "landscape", 0.0);
				}
			}
		}
